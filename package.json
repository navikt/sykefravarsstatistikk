{
  "name": "sykefravarsstatistikk",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "/sykefravarsstatistikk",
  "private": true,
  "scripts": {
    "ins": "yarn install && cd server && yarn install",
    "start": "craco start",
    "mock": "REACT_APP_MOCK=true yarn start",
    "dev": "yarn mock",
    "serve": "NODE_ENV=development node server/build/server.js",
    "build": "craco build && cd server && tsc",
    "test": "craco test ./",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "msw:init": "msw init ./public"
  },
  "dependencies": {
    "@grafana/faro-react": "^1.1.2",
    "@grafana/faro-web-sdk": "^1.1.2",
    "@grafana/faro-web-tracing": "^1.1.3",
    "@navikt/aksel-icons": "^4.7.0",
    "@navikt/arbeidsgiver-notifikasjon-widget": "^6.2.3",
    "@navikt/bedriftsmeny": "^6.10.0",
    "@navikt/ds-css": "^4.7.0",
    "@navikt/ds-icons": "^3.4.3",
    "@navikt/ds-react": "^4.7.0",
    "@navikt/fnrvalidator": "^1.3.0",
    "@navikt/ia-metrikker-client": "1.9.0",
    "@navikt/nav-dekoratoren-moduler": "^1.6.9",
    "@testing-library/react-hooks": "8.0.1",
    "amplitude-js": "^8.21.9",
    "browserslist": "^4.21.9",
    "classnames": "^2.3.2",
    "core-js": "^3.31.1",
    "pino": "^8.14.1",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-to-print": "^2.14.13",
    "recharts": "2.7.2",
    "swr": "^2.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@navikt/arbeidsgiver-notifikasjoner-brukerapi-mock": "^6.2.3",
    "@navikt/eslint-config-teamia": "^1.0.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/amplitude-js": "^8.16.2",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.3",
    "@types/jest-axe": "^3.5.5",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/recharts": "^2.0.0",
    "jest-axe": "^8.0.0",
    "msw": "1.2.1",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "@navikt/eslint-config-teamia"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "@next/next/no-html-link-for-pages": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
